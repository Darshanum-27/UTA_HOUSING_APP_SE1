<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".confirmwesthall">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/imageView27"
                android:layout_width="match_parent"
                android:layout_height="142dp"
                app:srcCompat="@drawable/housing_tour_portal" />

            <TextView
                android:id="@+id/textView57"
                android:layout_width="match_parent"
                android:layout_height="69dp"
                android:fontFamily="sans-serif-condensed-medium"
                android:paddingLeft="30sp"
                android:paddingTop="20sp"
                android:paddingEnd="30sp"
                android:text="CONFIRMATION PAGE"
                android:textColor="#EDE9E9"
                android:textSize="24sp" />

            <ImageView
                android:id="@+id/imageView20"
                android:layout_width="match_parent"
                android:layout_height="306dp"
                android:layout_marginTop="20sp"
                tools:srcCompat="@tools:sample/avatars" />

            <TextView
                android:id="@+id/finaloutput"
                android:layout_width="match_parent"
                android:layout_height="558dp"
                android:fontFamily="sans-serif-condensed-medium"
                android:paddingLeft="30sp"
                android:paddingTop="20sp"
                android:textColor="#F6F3F3"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/textView61"
                android:layout_width="match_parent"
                android:layout_height="62dp"
                android:fontFamily="sans-serif-condensed-medium"
                android:paddingLeft="30sp"
                android:paddingTop="20sp"
                android:paddingRight="20sp"
                android:text="Return To Home Page"
                android:textColor="#FAF8F8"
                android:textSize="20sp" />

            <Button
                android:id="@+id/button7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30sp"
                android:layout_marginTop="20sp"
                android:layout_marginRight="30sp"
                android:layout_marginBottom="40sp"
                android:onClick="returnhomepage"
                android:text="Return to Home Page" />

            <TextView
                android:id="@+id/textView67"
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:fontFamily="sans-serif-condensed-medium"
                android:paddingLeft="30sp"
                android:paddingTop="20sp"
                android:text="To Logout"
                android:textColor="#F4F2F2"
                android:textSize="20sp" />

            <Button
                android:id="@+id/button10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30sp"
                android:layout_marginTop="20sp"
                android:layout_marginRight="30sp"
                android:layout_marginBottom="30sp"
                android:onClick="signinpage"
                android:text="Logout" />

            <TextView
                android:id="@+id/textView59"
                android:layout_width="match_parent"
                android:layout_height="95dp"
                android:fontFamily="sans-serif-condensed-medium"
                android:paddingLeft="30sp"
                android:paddingTop="20sp"
                android:paddingRight="30sp"
                android:text="About US"
                android:textColor="#ECE5E5"
                android:textSize="24sp" />

            <ImageView
                android:id="@+id/imageView32"
                android:layout_width="match_parent"
                android:layout_height="103dp"
                app:srcCompat="@drawable/logo" />

            <TextView
                android:id="@+id/textView45"
                android:layout_width="match_parent"
                android:layout_height="381dp"
                android:background="#4297BD"
                android:paddingLeft="20sp"
                android:paddingTop="30sp"
                android:text="
    HOUSING INFORMATION\n
    \t ________________________________\n
    University Housing Office:\n
    University Center Suite 150\n
    301 W First Street\n
    Arlington, Texas 76019\n\n

    CONTACT US\n
    \t ________________________________\n
    Housing: 817-272-2791\n
    Housing@uta.edu\n
    Apartment and Res Life: 817-272-2926\n
    livingoncampus@uta.edu\n

    "
                android:textColor="#F6F5F5"
                android:textSize="20dp" />
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>